  <eClassifiers xsi:type="ecore:EClass" name="SimpleEdgeOperation" eSuperTypes="#//EdgeOperational"/>
  <eClassifiers xsi:type="ecore:EClass" name="CompositionalEdgeOperational" eSuperTypes="#//EdgeOperational">
