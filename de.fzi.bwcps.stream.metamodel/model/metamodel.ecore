  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="SinkNodeType" eSuperTypes="#//NodeType"/>
  <eClassifiers xsi:type="ecore:EClass" name="SourceNodeType" eSuperTypes="#//NodeType"/>
  <eClassifiers xsi:type="ecore:EClass" name="SinkSourceNodeType" eSuperTypes="#//SourceNodeType #//SinkNodeType"/>
  <eClassifiers xsi:type="ecore:EClass" name="StreamRepository" eSuperTypes="#//NamedElement">
