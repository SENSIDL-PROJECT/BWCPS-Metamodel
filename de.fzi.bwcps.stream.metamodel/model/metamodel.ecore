  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Scalar" eSuperTypes="#//OperationalElement"/>
  <eClassifiers xsi:type="ecore:EClass" name="Vector" eSuperTypes="#//OperationalElement">
