<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="bwcps_streams" nsURI="http://www.fzi.de/bwcps/stream/1.0" nsPrefix="streams">
  <eClassifiers xsi:type="ecore:EClass" name="Stream" eSuperTypes="#//NamedElement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="links" lowerBound="1" upperBound="-1"
        eType="#//NodeLink" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Node" abstract="true" eSuperTypes="#//NamedElement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="nodeType" lowerBound="1"
        eType="#//NodeType"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="NodeLink" abstract="true">
    <eStructuralFeatures xsi:type="ecore:EReference" name="source" lowerBound="1"
        eType="#//Node"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="sink" lowerBound="1" eType="#//Node"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="PrimitiveNodeLink" eSuperTypes="#//NodeLink">
    <eStructuralFeatures xsi:type="ecore:EReference" name="operational" eType="#//EdgeOperational"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="AggregatedNodeLink" eSuperTypes="#//NodeLink">
    <eStructuralFeatures xsi:type="ecore:EReference" name="aggregatedLinks" lowerBound="1"
        upperBound="-1" eType="#//NodeLink"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="NodeType" eSuperTypes="#//NamedElement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="refines" upperBound="-1"
        eType="#//NodeType"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="dataType" lowerBound="1"
        eType="ecore:EClass platform:/plugin/de.fzi.sensidl.design/model/sensIDL.ecore#//dataRepresentation/Data"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="NamedElement">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="SinkNodeType" eSuperTypes="#//NodeType"/>
  <eClassifiers xsi:type="ecore:EClass" name="SourceNodeType" eSuperTypes="#//NodeType"/>
  <eClassifiers xsi:type="ecore:EClass" name="SinkSourceNodeType" eSuperTypes="#//SourceNodeType #//SinkNodeType"/>
  <eClassifiers xsi:type="ecore:EClass" name="StreamRepository" eSuperTypes="#//NamedElement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="streams" upperBound="-1"
        eType="#//Stream" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="nodes" upperBound="-1"
        eType="#//Node" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="nodeTypes" upperBound="-1"
        eType="#//NodeType" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="EdgeNode" eSuperTypes="#//Node"/>
  <eClassifiers xsi:type="ecore:EClass" name="EdgeGateway" eSuperTypes="#//Node"/>
  <eClassifiers xsi:type="ecore:EClass" name="DataSink" eSuperTypes="#//Node"/>
  <eClassifiers xsi:type="ecore:EClass" name="EdgeOperationRepository">
    <eStructuralFeatures xsi:type="ecore:EReference" name="operations" upperBound="-1"
        eType="#//EdgeOperation" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="domains" upperBound="-1"
        eType="#//Domain" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="EdgeOperation" abstract="true" eSuperTypes="#//NamedElement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="domain" lowerBound="1"
        eType="#//Domain"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="codomain" lowerBound="1"
        eType="#//Domain"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Domain" eSuperTypes="#//NamedElement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="subDomains" upperBound="-1"
        eType="#//Domain"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="dimensions" lowerBound="1"
        upperBound="-1" eType="#//NodeType"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="OperationalElement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="domain" lowerBound="1"
        eType="#//Domain"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="values" lowerBound="1"
        upperBound="-1" eType="#//OperationalValue" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="EdgeOperational" abstract="true">
    <eStructuralFeatures xsi:type="ecore:EReference" name="input" lowerBound="1" eType="#//OperationalElement"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="mappingRule" lowerBound="1"
        eType="#//EdgeOperation"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="output" lowerBound="1"
        eType="#//OperationalElement" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="BlackBoxMapping" eSuperTypes="#//Mapping"/>
  <eClassifiers xsi:type="ecore:EClass" name="DeclarativeMapping" eSuperTypes="#//Mapping #//Selection">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="functionalExpression" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="AtomicEdgeOperation" eSuperTypes="#//EdgeOperational"/>
  <eClassifiers xsi:type="ecore:EClass" name="CompositionalEdgeOperational" eSuperTypes="#//EdgeOperational">
    <eStructuralFeatures xsi:type="ecore:EReference" name="operational" lowerBound="1"
        eType="#//EdgeOperational"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="OperationalDefintions">
    <eStructuralFeatures xsi:type="ecore:EReference" name="operationals" upperBound="-1"
        eType="#//EdgeOperational" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="imports" upperBound="-1"
        eType="#//OperationalDefintions"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Mapping" eSuperTypes="#//EdgeOperation"/>
  <eClassifiers xsi:type="ecore:EClass" name="Aggregation" eSuperTypes="#//EdgeOperation">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="size" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="joinOperation" lowerBound="1"
        eType="#//JoinOperation" defaultValueLiteral="NONE"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="step" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="OperationalValue">
    <eStructuralFeatures xsi:type="ecore:EReference" name="referenceDomain" lowerBound="1"
        eType="ecore:EClass platform:/plugin/de.fzi.sensidl.design/model/sensIDL.ecore#//dataRepresentation/Data"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="value" lowerBound="1" eType="ecore:EEnum platform:/plugin/de.fzi.sensidl.design/model/sensIDL.ecore#//dataRepresentation/DataType"
        defaultValueLiteral="UNDEFINED"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="JoinOperation">
    <eLiterals name="SUM"/>
    <eLiterals name="PRODUCT" value="1"/>
    <eLiterals name="MIN" value="2"/>
    <eLiterals name="MAX" value="3"/>
    <eLiterals name="NONE" value="4"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Selection" eSuperTypes="#//EdgeOperation"/>
  <eClassifiers xsi:type="ecore:EClass" name="Projection" eSuperTypes="#//EdgeOperation"/>
</ecore:EPackage>
